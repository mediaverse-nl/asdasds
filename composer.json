{
    "name": "Mediaverse/Test",
    "type": "library",
    "description": "test",
    "keywords": [
        "Mediaverse",
        "Test"
    ],
    "homepage": "https://github.com/mediaverse-nl/asdasds",
    "license": "MIT",
    "authors": [
        {
            "name": "Deveron Reniers",
            "email": "deveron.reniers@gmail.com",
            "homepage": "https://github.com/mediaverse-nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Mediaverse\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediaverse\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
